   "source": [
    "import scipy.integrate as integrate\n",
    "import scipy.special as special"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Energy(u):\n",
    "    energy=np.zeros(Nouts)\n",
    "    n=0\n",
    "    for timepoints in t:\n",
    "        if n==200:\n",
    "            break\n",
    "        energy[n]=integrate.trapz(u[n,:]**2,dx=0.01)\n",
    "        n+=1\n",
    "        energy=energy[:output_number]\n",
    "    return energy\n",
    "energy = Energy(solution)"
   ]
  },
